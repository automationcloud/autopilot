    "scripts": {
        "clean": "rm -rf ./dist ./out *.tsbuildinfo",
        "start": "electron out/main",
        "dev": "NODE_ENV=development run-p start webpack-hmr",
        "webpack-watch": "webpack --watch",
        "webpack-hmr": "webpack-dev-server --hot",
        "build": "run-p build:*",
        "build:main": "tsc -p tsconfig.main.json",
        "build:app": "NODE_ENV=production webpack",
        "dist": "run-s clean build dist:all",
        "dist:win-dir": "npm run _eb -- -w --dir && cd dist && mv win-unpacked Autopilot && zip -9 -r Autopilot-${npm_package_version}.zip Autopilot/*",
        "dist:all": "npm run _eb -- -w -m -l -p always",
        "dist:dev": "npm run _eb -- -m -p never",
        "_eb": "node ./patch-electron-builder && electron-builder",
        "slack:public": "node slack.js"
    },
